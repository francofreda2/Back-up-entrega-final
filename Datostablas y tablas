CREATE DEFINER=`root`@`localhost` FUNCTION `Distribucion_profesores`(
	Hora int (10)
) RETURNS varchar(40) CHARSET utf8mb4
    DETERMINISTIC
BEGIN
    DECLARE MomentoDelDia varchar(20);

    IF Hora > 7 THEN
		SET MomentoDelDia = 'Ma;ana';
    ELSEIF (credit >= 13 AND 
			credit <= 19) THEN
        SET MomentoDelDia = 'Tarde';
    ELSEIF (credit >= 20 AND 
			credit <= 24) THEN
        SET MomentoDelDia = 'Noche';
    END IF;
	-- return the customer level
	RETURN (MomentoDelDia);
END ;;
DELIMITER ;
