CREATE TABLE `promociones` (
  `id_promocion` int unsigned NOT NULL AUTO_INCREMENT,
  `id_socio` int unsigned NOT NULL,
  `Cod_promocion` int DEFAULT NULL,
  `Nombre_promocion` varchar(20) DEFAULT NULL,
  `Tel_socio` decimal(20,0) DEFAULT NULL,
  `Medio_de_pago_aceptado` varchar(20) DEFAULT NULL,
  `Banco_aceptado` varchar(20) DEFAULT NULL,
  PRIMARY KEY (`id_promocion`),
  KEY `id_socio` (`id_socio`),
  CONSTRAINT `promociones_ibfk_1` FOREIGN KEY (`id_socio`) REFERENCES `socios` (`id_socio`)
) ENGINE=InnoDB AUTO_INCREMENT=7 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
