CREATE TABLE `profesores` (
  `id_profesor` int unsigned NOT NULL AUTO_INCREMENT,
  `Cod_profesor` int DEFAULT NULL,
  `Dni_profesor` decimal(40,0) DEFAULT NULL,
  `Nombre_profesor` char(20) DEFAULT NULL,
  `Apellido_profesor` char(20) DEFAULT NULL,
  `Tel_profesor` decimal(40,0) DEFAULT NULL,
  `Direccion_profesor` char(40) DEFAULT NULL,
  `Certificado_medico_activo` char(20) DEFAULT NULL,
  `Sucursal_trabaja` varchar(20) DEFAULT NULL,
  `Obra_social_profesor` varchar(20) DEFAULT NULL,
  `Actividad_que_realiza` varchar(20) DEFAULT NULL,
  `id_sucursal` int unsigned DEFAULT NULL,
  PRIMARY KEY (`id_profesor`),
  KEY `id_sucursal` (`id_sucursal`),
  CONSTRAINT `profesores_ibfk_1` FOREIGN KEY (`id_sucursal`) REFERENCES `sucursal` (`id_sucursal`)
) ENGINE=InnoDB AUTO_INCREMENT=10 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

